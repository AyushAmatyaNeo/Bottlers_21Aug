
CREATE TABLESPACE HRIS_JWL_TABLESPACE DATAFILE 'D:\HRIS_JWL_TABLESPACE_DATAFILE.DBF' SIZE 10M EXTENT MANAGEMENT LOCAL AUTOALLOCATE;

CREATE USER HRIS_JWL IDENTIFIED BY HRIS_JWL DEFAULT TABLESPACE HRIS_JWL_TABLESPACE QUOTA 10M ON HRIS_JWL_TABLESPACE;



CREATE TABLESPACE MODERN_SLIPPER DATAFILE 'modern_slipper.dat' SIZE 10M REUSE AUTOEXTEND ON NEXT 10M;
CREATE USER HRIS_MODERN IDENTIFIED BY HRIS_MODERN DEFAULT TABLESPACE MODERN_SLIPPER QUOTA 10M ON MODERN_SLIPPER;